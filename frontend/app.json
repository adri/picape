{
  "expo": {
    "name": "Picape",
    "slug": "picape",
    "platforms": ["web", "ios"],
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "picape",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "adrian-cp",
            "project": "picape-mobile",
            "authToken": "56f87e47a8004b559e72468fdeb950a810adae68d1eb4dfe83e0e3f646a9582a"
          }
        }
      ]
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.adri.picape"
    }
  }
}
